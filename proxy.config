[
	{
		"hostnames": [
			"127.0.0.1",
			"http://127.0.0.1"
		],
		"content": [
			{
			"type": "file_system",
			"match": "/",
			"path": "/var/www/somefolder",
			"cachestrategy": "lru",
			"cachelimit": 2048
			}
		]
	},
	{
	"hostnames": [
			"localhost",
			"http://localhost",
			"default"
		],
		"content": [
			{
			"type": "unix_socket",
			"match": "/unix",
			"path": "/tmp/temp.sock"
			},
			{
			"type": "http_socket",
			"match": "/http",
			"path": "http://localhost:4000"
			},
			{
				"type": "file_system",
				"match": "/",
				"path": "/var/www/someother",
				"cachestrategy": "lru",
				"cachelimit": 2048,
				"compression": true,
				"defaultfile": "index.html",
				"defaultextension": ".html",
				"error": {
					"40x": "/error/40x.html",
					"401": "/error/401.html"
				}
			}
		]
	}
]
